        <Button x:Name="Return" Content="Назад" HorizontalAlignment="Left" Margin="29,17,0,0" VerticalAlignment="Top" Click="Return_Click" Style="{DynamicResource RoundedButton}" Height="31" Width="64"/>
        <ListBox Style="{DynamicResource ListBoxStyle1}" SelectionMode="Multiple" 
                 x:Name="seats"
                 
                 HorizontalAlignment="Left" 
                 Height="467" 
                 Margin="122,125,0,0" 
                 VerticalAlignment="Top" 
                 Background="#D3BD9A"
                 Width="1004" 
                 SelectionChanged="seats_SelectionChanged"
                 ScrollViewer.HorizontalScrollBarVisibility="Disabled">

            <ListBox.ItemContainerStyle>
                <Style  TargetType="{x:Type ListBoxItem}">
                    <Setter Property="SnapsToDevicePixels" Value="True"/>
                    <Setter Property="Padding" Value="4,1"/>
                    <Setter Property="HorizontalContentAlignment" Value="{Binding HorizontalContentAlignment, RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}"/>
                    <Setter Property="VerticalContentAlignment" Value="{Binding VerticalContentAlignment, RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}"/>
                    <Setter Property="Background" Value="Transparent"/>
                    <Setter Property="BorderBrush" Value="Transparent"/>
                    <Setter Property="BorderThickness" Value="0"/>
                    <Setter Property="Width" Value="55"/>
                    <Setter Property="Height" Value="56"/>
                    <Setter Property="HorizontalContentAlignment" Value="Center"/>
                    <Setter Property="Foreground" Value="Black"/>
                    <Setter Property="FontSize" Value="11"/>
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="{x:Type ListBoxItem}">
                                <Grid>
                                    <Border x:Name="Bd" 
                                        Margin="0" 
                                       
                                        CornerRadius="5"
                                        Background="{TemplateBinding Background}" 
                                        BorderBrush="{TemplateBinding BorderBrush}" 
                                        BorderThickness="{TemplateBinding BorderThickness}"
                                        Padding="{TemplateBinding Padding}" 
                                        SnapsToDevicePixels="true">
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition/>
                                                <RowDefinition Height="Auto"/>
                                            </Grid.RowDefinitions>
                                            <Image Source="{StaticResource seatFreeImage}"/>
                                            <ContentPresenter Grid.Row="1"
                                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" 
                                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                                        </Grid>
                                    </Border>
                                </Grid>

                                <ControlTemplate.Triggers>
                                    <MultiTrigger>
                                        <MultiTrigger.Conditions>
                                            <Condition Property="IsMouseOver" Value="True"/>
                                        </MultiTrigger.Conditions>
                                        <Setter TargetName="Bd" Property="Background" Value="Gray"/>
                                    </MultiTrigger>
                                    <MultiTrigger>
                                        <MultiTrigger.Conditions>
                                            <Condition Property="Selector.IsSelectionActive" Value="False"/>
                                            <Condition Property="IsSelected" Value="True"/>
                                        </MultiTrigger.Conditions>

                                    </MultiTrigger>
                                    <MultiTrigger>
                                        <MultiTrigger.Conditions>
                                            <Condition Property="Selector.IsSelectionActive" Value="True"/>
                                            <Condition Property="IsSelected" Value="True"/>
                                        </MultiTrigger.Conditions>
                                        <Setter TargetName="Bd" Property="Background" Value="#E83A14"/>
                                    </MultiTrigger>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter Property="TextElement.Foreground" TargetName="Bd" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </ListBox.ItemContainerStyle>
            <ListBox.ItemsPanel>
                <ItemsPanelTemplate>
                    <WrapPanel/>
                </ItemsPanelTemplate>
            </ListBox.ItemsPanel>

        </ListBox>
        <DatePicker x:Name="date" IsTodayHighlighted="True" Style="{DynamicResource DatePickerStyle1}" HorizontalAlignment="Left" Margin="1000,43,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="ChooseHall" HorizontalAlignment="Left" Margin="734,77,0,0" VerticalAlignment="Top" Width="190" DropDownClosed="ChooseHall_DropDownClosed"/>
        <Label Content="Зал" HorizontalAlignment="Left" Margin="698,74,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="ChooseCinema" HorizontalAlignment="Left" Margin="735,44,0,0" VerticalAlignment="Top" Width="246" SelectionChanged="ChooseCinema_SelectionChanged"/>
        <Label Content="Выберите кинотеатр" HorizontalAlignment="Left" Margin="608,43,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TypeHallCinema" HorizontalAlignment="Left" Margin="938,80,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120" TextChanged="TypeHallCinema_TextChanged" IsReadOnly="True"/>
        <Button x:Name="booked" Content="Забронировать" FontSize="18" HorizontalAlignment="Left" Margin="534,625,0,0" VerticalAlignment="Top" Height="48" Width="174" Style="{DynamicResource RoundedButton}" Click="booked_Click"/>
        <ComboBox x:Name="ChooseFilmCombobox" HorizontalAlignment="Left" Margin="249,44,0,0" VerticalAlignment="Top" Width="350" Height="28"/>
        <Label Content="Выберите фильм" HorizontalAlignment="Left" Margin="135,46,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="SelectedSeat" IsReadOnly="True" HorizontalAlignment="Left" Margin="244,635,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="241" Height="34"/>
        <Label x:Name="SelectedSeatText" Content="Выбранные места" HorizontalAlignment="Left" Margin="132,634,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="Select_ID_seat" IsReadOnly="True" Visibility="Hidden"  HorizontalAlignment="Left" Margin="1146,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="27"/>
        <TextBox x:Name="Select_ID_Film" IsReadOnly="True" Visibility="Hidden"  HorizontalAlignment="Left" Margin="1146,174,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="27"/>
        <TextBox x:Name="Select_ID_Cinema" IsReadOnly="True" Visibility="Hidden"  HorizontalAlignment="Left" Margin="1147,218,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="27"/>
        <TextBox x:Name="Select_DateFilmSession" IsReadOnly="True" Visibility="Hidden"  HorizontalAlignment="Left" Margin="1147,260,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="27"/>
        <TextBox x:Name="Select_IDFilmSession" IsReadOnly="True" Visibility="Hidden"  HorizontalAlignment="Left" Margin="1145,304,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="27"/>
        <TextBox x:Name="Select_Price" IsReadOnly="True" Visibility="Hidden"  HorizontalAlignment="Left" Margin="1143,345,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="27"/>
        <TextBox x:Name="Select_TimeFilmSession" IsReadOnly="True" Margin="1145,320,0,0" Width="120" Height="27" Visibility="Hidden"/>
        <xctk:TimePicker x:Name="Time" HorizontalAlignment="Left" Margin="1120,38,0,0" VerticalAlignment="Top" Height="27" Width="113"/>

    </Grid>
</Window>
